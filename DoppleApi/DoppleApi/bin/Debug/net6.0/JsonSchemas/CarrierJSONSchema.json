{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "tagId": {
        "type": "string",
        "minLength": 1,
        "maxLength": 5,
        "pattern": "([0-9])*"
    },
    "orderId": {
        "type": "string",
        "minLength": 1,
        "pattern": "([0-9])*"
    },
    "stationId": {
        "type": "integer",
        "minimum": 0,
        "maximum": 999
    },
    "statusCarrier": {
      "type": "string"
    }
  },
  "required": [
    "tagId",
    "orderIdO",
    "stationId",
    "statusCarrier"
  ]
}