{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "tagId": {
        "type": "string",
        "minLength": 1,
        "maxLength": 5,
        "pattern": "([0-9])*"
    },
    "testId": {
        "type": "integer",
        "minimum": 1
    },
    "orderId": {
        "type": "string",
        "minLength": 1,
        "pattern": "([0-9])*"
    }
  },
  "required": [
    "tagId",
    "testId",
    "orderId"
  ]
}
